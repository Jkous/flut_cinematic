{
  "search.exclude": {
    "**/*.g.dart": true,
    "**/*.freezed.dart": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "files.defaultLanguage": "dart",
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/*.freezed.dart": true,
    "**/*.g.dart": false,
    "**/.flutter-plugins*": true,
    "**/.metadata*": true,
    "**/.packages": true
  }
}